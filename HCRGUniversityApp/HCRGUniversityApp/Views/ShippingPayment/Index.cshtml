            var end = new Date(dateAr[1] + '/' + dateAr[0] + '/' + dateAr[2]);
            end = end.setSeconds(end.getSeconds());
            
            var _second = 1000;
            var _minute = _second * 60;
            var _hour = _minute * 60;
            var _day = _hour * 24;
            var timer;
            var minutes ;
            var seconds ;
            
            //var now = new Date();

            var st = '@ViewBag.StartDate';

        var dateAr1 = st.split('-');
