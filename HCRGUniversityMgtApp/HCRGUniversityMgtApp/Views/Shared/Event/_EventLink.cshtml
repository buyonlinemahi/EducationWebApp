<div id="divInsurerSearchPartial">
    <form>
        <div class="panel-wrapper">
            <div class="panel panel-default">
                <!-- Default panel contents -->
                <div class="panel-heading" data-bind="text:'Search ' + selectedItem().LinkTypeName"></div>
                <div class="panel-body">
                    <div class="col-md-2"><label data-bind="text:selectedItem().LinkTypeValue"></label> </div>
                    <div class="col-md-10">
                        <div class="form-group col-md-10">
                            <input type="text" maxlength="50" name="searchText" id="SearchInsurerTitle" class="form-control" data-bind="value:SearchText,jqBootstrapValidation: {}" placeholder="Search" required="required">
                        </div>
                        <button class="btn btn-default text-right col-md-2" id="SearchInsurer" data-bind="click:SearchCourseOrNews">Search</button>
                    </div>
                </div>

                <div class="row">
                    <div>
                        <div class="col-sm-12">
                            <div class="col-sm-12">
                             
                                    <div class="panel-body" id="divsearchpanel">
                                        <div class="panel panel-default">
                                            <div class="panel-heading" data-bind="text: selectedItem().LinkTypeName+' Results'"></div>
                                            <!-- Table -->
                                            <!--ko if: selectedItem().LinkTypeValue== 'Article Name'-->
                                            <table class="table table-striped table-fixed-event">
                                                <thead>
                                                    <tr class="clearfix col-md-12 row">
                                                        <th class="col-md-8">Article Name</th>
                                                        <th class="col-md-4">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="col-md-12">
                                                    <!--ko if:NewsResults!= null-->
                                                    <!-- ko foreach:NewsResults -->
                                                    <tr class="clearfix col-md-12 row">
                                                        <td class="col-md-8"><span data-bind="text:NewsTitle" /></td>
                                                        <td class="col-md-4">
                                                            <a href="#" class="inner-content-link blue" style="color: #2b2b2b" data-bind="click:$root.LinkNewsTitle">Link </a>
                                                        </td>
                                                    </tr>
                                                    <!--/ko-->
                                                    <!--/ko-->
                                                </tbody>
                                                <tfoot>
                                                    <!--ko if:NewsResults== null || NewsResults().length==0 -->
                                                    <tr>
                                                        <td colspan="5">
                                                            <div class="alert alert-info">
                                                                <button type="button" class="close" data-dismiss="alert">
                                                                    &times;
                                                                </button>
                                                                No Result Found.
                                                            </div>
                                                        </td>
                                                    </tr>
                                                    <!-- /ko -->
                                                </tfoot>
                                            </table>
                                            <nav class="margin-left">
                                                @Html.Partial("~/Views/Shared/_PagingPopUp.cshtml")
                                            </nav>
                                            <!--/ko-->
                                            <!--ko if: selectedItem().LinkTypeValue== 'Course Name'-->
                                            <table class="table table-striped table-fixed-event">
                                                <thead>
                                                    <tr class="clearfix col-md-12 row">
                                                        <th class="col-md-8">Course Name</th>
                                                        <th class="col-md-4">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="col-md-12">
                                                    <!--ko if:EducationResults!= null-->
                                                    <!-- ko foreach:EducationResults -->
                                                    <tr class="row">
                                                        <td class="col-md-8"><span data-bind="text:CourseName" /></td>
                                                        <td class="col-md-4"><a href="#" class="inner-content-link blue" data-bind="click:$root.LinkCourseName" style="color: #808080;">Link</a></td>
                                                    </tr>
                                                    <!--/ko-->
                                                    <!--/ko-->
                                                </tbody>
                                                <tfoot>
                                                    <!--ko if:EducationResults== null || EducationResults().length==0 -->
                                                    <tr>
                                                        <td colspan="5">
                                                            <div class="alert alert-info">
                                                                <button type="button" class="close" data-dismiss="alert">
                                                                    &times;
                                                                </button>
                                                                No Result Found.
                                                            </div>
                                                        </td>
                                                    </tr>
                                                    <!-- /ko -->
                                                </tfoot>
                                            </table>
                                            <nav class="margin-left">
                                                @Html.Partial("~/Views/Shared/_PagingPopUp.cshtml")
                                            </nav>
                                            <!--/ko-->
                                        </div>
                                    </div>
                      
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
