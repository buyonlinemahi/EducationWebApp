

<div class="modal-body">
    <div class="panel-body">
        <div class="panel-default panel">
            <table class="table table-striped table-fixed-event">
                <thead>
                    <tr class="clearfix col-md-12 row">
                        <th class="col-md-3 paddingleft20">Purchase Item(s)
                            <input type="hidden" name="ppSkip" id="hidskip" data-bind="value: $root.ppSkip" />
                        </th>
                        
                    </tr>
                </thead>

                <tbody class="col-md-12">



                    <!--ko if:(ppTotalItemCount() > 0 || ppTotalItemCount() == 0 )-->
                    <!--ko if:ProductPurchaseResultList!= null-->
                    <!-- ko foreach:ProductPurchaseResultList -->
                    <tr class="clearfix col-md-12 row">
                        <td class="col-md-1" style="width: 2%;">
                            <span data-bind="text: $index() + 1" />
                        </td>
                        <td class="col-md-11" style="width: 98%;">
                            <span data-bind="text:ProductName" />
                        </td>
                    </tr>                   
                    <!--/ko-->
                    
                    <tr class="clearfix col-md-12 row">
                        <td class="col-md-12">
                            <nav class="margin-left">
                                <!-- ko if:ppPager().ppTotalItemCount() > 5 -->
                                <div class="pagination" data-bind="with: ppPager">
                                    <div class="pagexofx">
                                        <span data-bind="text:ppFirstItemIndex"></span>
                                        -
                                        <span data-bind="text: ppLastItemIndex"></span>
                                        of
                                        <strong>
                                            <span data-bind="text: ppTotalItemCount"></span>
                                        </strong>
                                        total results.
                                    </div>
                                    <ul class="pagination">
                                        <!-- ko if:ppCurrentPage() === 1 -->
                                        <li class="disabled">
                                            <a href="#" class="disabledPage">&laquo;&laquo;</a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko ifnot:ppCurrentPage() === 1 -->
                                        <li>
                                            <a href="#" data-bind="click: function(){ ppCurrentPage(1); }">&laquo;&laquo;</a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko if:ppHasPrevPage -->
                                        <li>
                                            <a href="#" data-bind="click: function(){ ppCurrentPage(ppCurrentPage() - 1); }">&laquo;</a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko ifnot:ppHasPrevPage -->
                                        <li class="disabled">
                                            <a href="#" class="disabledPage">&laquo;</a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko foreach:ppPages -->
                                        <!-- ko if:$data === $parent.ppCurrentPage() -->
                                        <li class="active">
                                            <span class="currentPage" data-bind="text: $data"></span>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko if:$data !== $parent.ppCurrentPage() -->
                                        <li>
                                            <a href="#" data-bind="text: $data, click: function(){ $parent.ppCurrentPage($data); }"></a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- /ko -->
                                        <!-- ko if:ppHasNextPage -->
                                        <li>
                                            <a href="#" data-bind="click: function(){ ppCurrentPage(ppCurrentPage() + 1); }">&raquo;</a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko ifnot:ppHasNextPage -->
                                        <li class="disabled">
                                            <a href="#" class="disabledPage">&raquo;</a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko ifnot:ppCurrentPage() === ppLastPage() -->
                                        <li>
                                            <a href="#" data-bind="click: function(){ ppCurrentPage(ppLastPage()); }">&raquo;&raquo;</a>
                                        </li>
                                        <!-- /ko -->
                                        <!-- ko if:ppCurrentPage() === ppLastPage() -->
                                        <li class="disabled">
                                            <a href="#" class="disabledPage">&raquo;&raquo;</a>
                                        </li>
                                        <!-- /ko -->
                                    </ul>

                                </div>
                                <!-- /ko -->
                               
                            </nav>
                        </td>
                    </tr>
                    <!--/ko-->
                    <!--/ko-->
                    <tr class="clearfix col-md-12 row">
                        <td class="col-md-3">    
                            <input type="hidden" name="ShippingPaymentID" data-bind="value:ShippingPaymentID" />                                                                         
                            <span>Name</span>
                            
                        </td>
                        <td class="col-md-9">

                            <span data-bind="text:SAFirstName() +' '+ SALastName()" />

                        </td>
                    </tr>

                    <tr class="clearfix col-md-12 row">

                        <td class="col-md-3"><span>Address</span></td>
                        <td class="col-md-9">
                            <span data-bind="text:SAAddress()" />

                        </td>
                    </tr>

                    <tr class="clearfix col-md-12 row">

                        <td class="col-md-3"><span>City</span></td>
                        <td class="col-md-9">
                            <span data-bind="text:SACity" />
                        </td>
                    </tr>

                    <tr class="clearfix col-md-12 row">

                        <td class="col-md-3"><span>State</span></td>
                        <td class="col-md-9">
                            <span data-bind="text:StateName"></span>
                        </td>
                    </tr>

                    <tr class="clearfix col-md-12 row">

                        <td class="col-md-3"><span>Zip</span></td>
                        <td class="col-md-9">
                            <span data-bind="text:SAPostalCode" />
                        </td>
                    </tr>

                    <tr class="clearfix col-md-12 row">

                        <td class="col-md-3"><span>Shipped Date</span>                         
                        </td>
                        <td class="col-md-9">
                            <input required="required" data-validation-regex-message="MM/DD/YYYY" data-validation-regex-regex="^(0[1-9]|1[012])[- /.](0[1-9]|[12][0-9]|3[01])[- /.](19|20)\d\d+$"
                                   id="ProductShippedOn" type="text" class="form-control datepicker"
                                   name="ProductShippedOn" placeholder="Shipping Date" style="width: 30%;">
                        </td>
                    </tr>

                    <tr class="clearfix col-md-12 row">

                        <td class="col-md-12">
                            <label class="font-16">
                                <button type="submit" class="btn btn-default">Save</button>                               
                            </label>
                        </td>
                    </tr>
                </tbody>

            </table>
        </div>

    </div>
</div>