
<div id="DivFacutlyDetails">

                   
                        <!-- Table -->
                        <table class="table table-striped table-fixed-event">
                            <thead>
                                <tr class="clearfix">
                                    <th class="col-md-3 col-sm-4">Headings</th>
                                    <th class="col-md-9 col-sm-8">Description</th>


                                </tr>
                            </thead>
                            <tbody>

                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>First Name</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:FirstName" />

                                    </td>
                                </tr>

                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Last Name</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:LastName" />

                                    </td>
                                </tr>

                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>E-mail</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:Email" />

                                    </td>
                                </tr>
                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Phone</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <!--ko if:Phone()!=null -->
                                        <span data-bind="formatNumberText:Phone"></span>
                                        <!--/ko-->

                                    </td>
                                </tr>
                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Company</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:Company" />

                                    </td>
                                </tr>

                              


                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Title</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:ProfessionalTitle" />

                                    </td>
                                </tr>


                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Address Street</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:AddressStreet" />

                                    </td>
                                </tr>


                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Apt | Suite | Floor</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:AddressFloor" />

                                    </td>
                                </tr>


                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>City</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:City" />

                                    </td>
                                </tr>


                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>State</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:State" />

                                    </td>
                                </tr>

                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Zip</span></td>
                                    <td class="col-md-9 col-sm-8">
                                        <span data-bind="text:Zip" />

                                    </td>
                                </tr>


                                @*<tr class="clearfix">

                                    <td class="col-md-3">
                                        <span>
                                            Area Of Expertise
                                        </span>
                                    </td>
                                    <td class="col-md-9">
                                        <span data-bind="text:AreaOfExpertise
" />

                                    </td>
                                </tr>

                                <tr class="clearfix">

                                    <td class="col-md-3"><span>Topics</span></td>
                                    <td class="col-md-9">
                                        <span data-bind="text:Topics" />

                                    </td>
                                </tr>*@

                                <tr class="clearfix">

                                    <td class="col-md-3 col-sm-4"><span>Resume</span></td>
                                    <td class="col-md-9 col-sm-8">
                                         <a data-bind="attr: { 'href': '@Url.Action("Download", "College")?ID='+FacultyID()}" style="color: blue;"><span class="btn btn-default">View</span></a>
                                     @*<a data-bind="attr:{href:ResumeLink,target:'_blank'}" style="color: blue;"><span class="btn btn-default">View</span></a>*@
                                       
                                    </td>
                                </tr>






                            </tbody>

                        </table>


</div>
